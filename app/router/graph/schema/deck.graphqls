type Deck {
  id: ID!
  userId: String!
  name: String!
  createdAt: Timestamp!
  updatedAt: Timestamp!
}

extend type Query {
  decks: [Deck!]! @hasRole(role: USER)
  deck(id: ID!): Deck! @hasRole(role: USER)
}

input CreateDeckInput {
  name: String!
}

extend type Mutation {
  createDeck(input: CreateDeckInput!): Deck! @hasRole(role: USER)
}
